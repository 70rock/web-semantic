    <PersonaHistoricaBoliviana rdf:about="http://example.org/personasHistoricasBolivianas#SimonBolivar">
        <nombre>Simón Bolívar</nombre>
        <resumen>Conocido como El Libertador, fue una de las figuras más destacadas de la emancipación americana frente al Imperio español. Contribuyó de manera decisiva a la independencia de Bolivia y otros países sudamericanos.</resumen>
        <fechaNacimiento rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1783-07-24</fechaNacimiento>
        <fechaFallecimiento rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1830-12-17</fechaFallecimiento>
        <lugarNacimiento rdf:resource="http://dbpedia.org/resource/Caracas"/>
        <lugarFallecimiento rdf:resource="http://dbpedia.org/resource/Santa_Marta"/>
        <nacionalidad rdf:resource="http://dbpedia.org/resource/Venezuela"/>
        <ocupacion rdf:resource="http://dbpedia.org/resource/Military_leader"/>
    </PersonaHistoricaBoliviana>

    <PersonaHistoricaBoliviana rdf:about="http://example.org/personasHistoricasBolivianas#AntonioJoseDeSucre">
        <nombre>Antonio José de Sucre</nombre>
        <resumen>Conocido como el Gran Mariscal de Ayacucho, fue un político, estadista y militar venezolano, prócer de la independencia americana. Fue el primer presidente de Bolivia.</resumen>
        <fechaNacimiento rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1795-02-03</fechaNacimiento>
        <fechaFallecimiento rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1830-06-04</fechaFallecimiento>
        <lugarNacimiento rdf:resource="http://dbpedia.org/resource/Cumaná"/>
        <lugarFallecimiento rdf:resource="http://dbpedia.org/resource/Berrucos"/>
        <nacionalidad rdf:resource="http://dbpedia.org/resource/Venezuela"/>
        <ocupacion rdf:resource="http://dbpedia.org/resource/Military_leader"/>
    </PersonaHistoricaBoliviana>

    <PersonaHistoricaBoliviana rdf:about="http://example.org/personasHistoricasBolivianas#AndresDeSantaCruz">
        <nombre>Andrés de Santa Cruz</nombre>
        <resumen>Fue un militar y político boliviano, presidente de Bolivia y de la Confederación Perú-Boliviana. Es considerado uno de los más importantes estadistas de la historia boliviana.</resumen>
        <fechaNacimiento rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1792-12-05</fechaNacimiento>
        <fechaFallecimiento rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1865-09-25</fechaFallecimiento>
        <lugarNacimiento rdf:resource="http://dbpedia.org/resource/La_Paz"/>
        <lugarFallecimiento rdf:resource="http://dbpedia.org/resource/Beauvoir-sur-Mer"/>
        <nacionalidad rdf:resource="http://dbpedia.org/resource/Bolivia"/>
        <ocupacion rdf:resource="http://dbpedia.org/resource/Politician"/>
    </PersonaHistoricaBoliviana>

    <PersonaHistoricaBoliviana rdf:about="http://example.org/personasHistoricasBolivianas#JuanaAzurduy">
        <nombre>Juana Azurduy de Padilla</nombre>
        <resumen>Fue una patriota y guerrillera del Alto Perú que luchó por la independencia de Bolivia y Argentina. Es considerada una heroína de la independencia americana.</resumen>
        <fechaNacimiento rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1780-07-12</fechaNacimiento>
        <fechaFallecimiento rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1862-05-25</fechaFallecimiento>
        <lugarNacimiento rdf:resource="http://dbpedia.org/resource/Chuquisaca"/>
        <lugarFallecimiento rdf:resource="http://dbpedia.org/resource/Sucre"/>
        <nacionalidad rdf:resource="http://dbpedia.org/resource/Bolivia"/>
        <ocupacion rdf:resource="http://dbpedia.org/resource/Military_leader"/>
    </PersonaHistoricaBoliviana>

    <PersonaHistoricaBoliviana rdf:about="http://example.org/personasHistoricasBolivianas#EduardoAvaroa">
        <nombre>Eduardo Abaroa</nombre>
        <resumen>Fue un héroe boliviano de la Guerra del Pacífico. Su sacrificio en la Batalla de Calama lo convirtió en uno de los mártires más importantes de Bolivia.</resumen>
        <fechaNacimiento rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1838-10-13</fechaNacimiento>
        <fechaFallecimiento rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1879-03-23</fechaFallecimiento>
        <lugarNacimiento rdf:resource="http://dbpedia.org/resource/San_Pedro_de_Atacama"/>
        <lugarFallecimiento rdf:resource="http://dbpedia.org/resource/Calama"/>
        <nacionalidad rdf:resource="http://dbpedia.org/resource/Bolivia"/>
